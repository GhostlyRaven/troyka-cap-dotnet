		<DebugSymbols>false</DebugSymbols>
		<DocumentationFile>D:\Users\DANTE\Desktop\TroykaCap.Expander\src\TroykaCap.Expander\TroykaCap.Expander\TroykaCap.Expander.xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>D:\Users\DANTE\Desktop\TroykaCap.Expander\src\TroykaCap.Expander\TroykaCap.Expander\TroykaCap.Expander.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Unosquare.Raspberry.Abstractions" Version="0.4.1" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Resources\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="Resources\Amperka.png">
